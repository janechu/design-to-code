{
  "name": "design-to-code",
  "description": "A set of utilities to assist in creating web UI",
  "sideEffects": false,
  "version": "1.0.0-alpha.3",
  "type": "module",
  "author": "Jane Chu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janechu/design-to-code.git"
  },
  "bugs": {
    "url": "https://github.com/janechu/design-to-code/issues/new/choose"
  },
  "main": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:message-system && npm run copy:css",
    "build:app": "npm run build:message-system && npm run build:examples",
    "build:examples": "cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --mode=production",
    "build:esm": "tsc --project ./tsconfig.esm.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:message-system": "webpack --config webpack.message-system.config.cjs",
    "clean:dist": "node ../../build/clean.js dist && node ../../build/clean.js .tmp",
    "convert:mdn-data": "node -r esm ./build/generate-mdn-data-files",
    "copy:css": "copyfiles -u 1 'src/**/*.css' dist/stylesheets/",
    "test:mdn-data": "node -r esm ./build/generate-mdn-data-files-test",
    "prepare": "npm run clean:dist && npm run build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.ts\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.ts\" --list-different",
    "start": "node ./www/server.cjs",
    "start:examples:form": "webpack serve --config app/examples/form/webpack.dev.cjs",
    "test": "npm run eslint && npm run test:playwright",
    "test:playwright": "npx playwright test",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "watch": "npm run build -- -w --preserveWatchOutput"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.4",
    "@types/webpack-env": "^1.15.2",
    "ajv": "^6.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.0.0",
    "eslint-config-prettier": "^6.10.1",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.0.0",
    "lodash-es": "4.17.21",
    "mdn-data": "^2.0.11",
    "mini-css-extract-plugin": "^2.0.0",
    "monaco-editor": "0.31.0",
    "monaco-editor-webpack-plugin": "^7.0.0",
    "prettier": "2.0.2",
    "resolve-typescript-plugin": "^1.1.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^4.0.1",
    "typescript": "~4.3.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.0.0",
    "worker-loader": "^3.0.0"
  },
  "peerDependencies": {
    "lodash-es": "^4.0.0"
  },
  "dependencies": {
    "@microsoft/fast-colors": "^5.3.1",
    "@microsoft/fast-element": "^2.0.0-beta.23",
    "@microsoft/fast-web-utilities": "^4.8.1",
    "vscode-html-languageservice": "^3.1.3",
    "vscode-web-custom-data": "^0.3.5"
  }
}
